  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/chokidar": "^1.7.5",
    "@types/eslint": "^9.6.1",
    "@types/estree-jsx": "^1.0.5",
    "@types/jest": "^29.5.3",
    "@types/micromatch": "^4.0.9",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "8.23.0",
